{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "__API_URL__/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 10,
        "maxAge": "1h",
        "timeout": "5s"
      }
    },
    {
      "name": "ws",
      "urls": [
        "__WS_URL__/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 10,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/offline",
    "!/assets/**"
  ],
  "offlineUrls": [
    "/offline.html"
  ]
}
